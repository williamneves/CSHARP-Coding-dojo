@model ListOfLeagues



@{
    <ul class="list-group list-group-flush p-1 mb-3 col-6">
    <li class="list-group-item active p-1" aria-current="true">[R]... leagues:</li>
    @foreach(League league in @Model.Leagues)
    {

        <li class="list-group-item p-1"> @league.Name</li>
    }
    </ul>
}