@model ListOfTeams



@{
    <ul class="list-group list-group-flush p-1 mb-3 col-6">
    <li class="list-group-item active p-1" aria-current="true">[R]...Teams:</li>
    @foreach(Team teams in @Model.Teams)
    {

        <li class="list-group-item p-1">@teams.Location @teams.TeamName</li>
    }
    </ul>
}