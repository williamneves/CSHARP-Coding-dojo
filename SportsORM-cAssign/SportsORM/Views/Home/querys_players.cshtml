@model ListOfPlayers



@{
    <ul class="list-group list-group-flush p-1 mb-3 col-6">
    <li class="list-group-item active p-1" aria-current="true">[R]...all womens' leagues</li>
    @foreach(Player player in @Model.Players)
    {

        <li class="list-group-item p-1">@player.LastName @player.FirstName</li>
    }
    </ul>
}